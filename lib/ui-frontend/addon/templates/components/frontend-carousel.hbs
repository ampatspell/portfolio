<div class="translate" style={{translate}}>
  {{#each images as |image|}}
    <div class="image" style={{imageStyle}} {{action 'next'}}>
      {{frontend-image src=image.attachments.original.url maxWidthSelector='.frontend-section'}}
      {{frontend-gallery-image-description image=image class='description'}}
    </div>
  {{/each}}
</div>
