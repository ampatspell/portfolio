{{#if section.isLink}}
  <div class="title"><a href={{section.url}} rel="noopener noreferrer" target={{if section.tab '_blank'}}>{{section.menuTitle_}}</a></div>
{{else}}
  <div class="title">{{#link-to 'sections.section' section}}{{section.menuTitle_}}{{/link-to}}</div>
{{/if}}
{{#if section.sortedVisibleSections}}
  <div class="nested">
    {{#each section.sortedVisibleSections as |section|}}
      {{frontend-menu-item section=section}}
    {{/each}}
  </div>
{{/if}}
