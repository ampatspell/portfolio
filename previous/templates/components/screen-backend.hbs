{{#ui-backend as |backend|}}

  {{#backend.navigation}}
    {{#ui-backend-navigation as |nav|}}
      {{#each navigationItems as |item|}}
        {{nav.item icon=item.icon selected=item.selected route=item.route}}
      {{/each}}
    {{/ui-backend-navigation}}
  {{/backend.navigation}}

  {{#backend.content}}
    {{yield}}
  {{/backend.content}}

{{/ui-backend}}
